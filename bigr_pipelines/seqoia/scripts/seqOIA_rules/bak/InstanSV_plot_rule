rule InstanSV_plot:
	input:
		"{basedir}/{analysis}/{prefix}/CNVnator/instanSV/"
	output:
		"{basedir}/{analysis}/{prefix}/CNVnator/instanSV/{prefix}_instanSV.pdf"
	params:
		genome_options = config["InstanSV"]["OPTIONS"]
	log:
		"{basedir}/{analysis}/log/{prefix}/{prefix}_instanSV.log"
	shell:
		"/usr/bin/cnvnator2intansv.R --cnvnator={input} --genome={params.genome_options} --pdf={output} 2>&1 | tee -a {log}"
